{% extends 'base.html.twig' %}

{% block body %}
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
    </tr>
  </thead>
  <tbody>
  {% for category in categories %}

    <tr>
      <th scope="row"> <a href="{{ path('category_show', {'id': category.id})}}"> {{category.id}} </a></th>
      <th scope="row">{{category.name}}</th>
    </tr>
    {% endfor %}
  </tbody>

{% endblock %}
